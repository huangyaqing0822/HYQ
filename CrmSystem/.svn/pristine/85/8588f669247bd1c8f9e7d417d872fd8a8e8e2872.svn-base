package com.lxit.crmsystem.entity;



/**
 * @author Administrator
 * 角色类
 *
 */
public class Role {
	private Integer roleId;//角色Id
	private String roleName;//角色名称
	private String roleCoding;//角色编码
	private Integer departmentId;//所属部门Id
	private String roleRemark;//角色备注
	private String roleUdatTime;//最后修改时间
	private Integer roleUpdateSid;//最后修改的人的ID
	private Integer roleNid;//上司的ID
	private String staffName;
	private String departmentName;
	
	
	
	
	public Integer getRoleNid() {
		return roleNid;
	}
	public void setRoleNid(Integer roleNid) {
		this.roleNid = roleNid;
	}
	public Integer getRoleUpdateSid() {
		return roleUpdateSid;
	}
	public void setRoleUpdateSid(Integer roleUpdateSid) {
		this.roleUpdateSid = roleUpdateSid;
	}
	public String getStaffName() {
		return staffName;
	}
	public void setStaffName(String staffName) {
		this.staffName = staffName;
	}
	public String getRoleUdatTime() {
		return roleUdatTime;
	}
	public void setRoleUdatTime(String roleUdatTime) {
		this.roleUdatTime = roleUdatTime;
	}
	public String getDepartmentName() {
		return departmentName;
	}
	public void setDepartmentName(String departmentName) {
		this.departmentName = departmentName;
	}
	public Role() {
		super();
	}
	public Integer getRoleId() {
		return roleId;
	}
	public void setRoleId(Integer roleId) {
		this.roleId = roleId;
	}
	public String getRoleName() {
		return roleName;
	}
	public void setRoleName(String roleName) {
		this.roleName = roleName;
	}
	public String getRoleCoding() {
		return roleCoding;
	}
	public void setRoleCoding(String roleCoding) {
		this.roleCoding = roleCoding;
	}
	public Integer getDepartmentId() {
		return departmentId;
	}
	public void setDepartmentId(Integer departmentId) {
		this.departmentId = departmentId;
	}
	public String getRoleRemark() {
		return roleRemark;
	}
	public void setRoleRemark(String roleRemark) {
		this.roleRemark = roleRemark;
	}
	@Override
	public String toString() {
		return "Role [roleId=" + roleId + ", roleName=" + roleName + ", roleCoding=" + roleCoding + ", departmentId="
				+ departmentId + ", roleRemark=" + roleRemark + ", roleUdatTime=" + roleUdatTime + ", roleUpdateSid="
				+ roleUpdateSid + ", roleNid=" + roleNid + ", staffName=" + staffName + ", departmentName="
				+ departmentName + "]";
	}

	
}
