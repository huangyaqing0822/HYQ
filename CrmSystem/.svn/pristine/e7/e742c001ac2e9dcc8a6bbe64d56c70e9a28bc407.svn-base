package com.lxit.crmsystem.vo;

public class Markets {

	private Integer marketId;//销售计划Id
	private String marketCoding;//销售计划编码
	private String marketName;//计划人姓名
	private String marketDate;//计划月份
	private String marketStateValue;//计划状态
	private String marketContent;//计划内容
	private String marketUpdateDate;//最后一次修改时间
	private String marketCreateDate;//创建时间
	private String marketOperatorName;//操作人
	private String marketWaithandleName;//待处理人
	private int marketUpdateSid;//操作人
	private String staffName;
	
	public Markets(){
		
	}

	@Override
	public String toString() {
		return "Markets [marketId=" + marketId + ", marketCoding=" + marketCoding + ", marketName=" + marketName
				+ ", marketDate=" + marketDate + ", marketStateValue=" + marketStateValue + ", marketContent="
				+ marketContent + ", marketUpdateDate=" + marketUpdateDate + ", marketCreateDate=" + marketCreateDate
				+ ", marketOperatorId=" + marketOperatorName + ", marketWaithandleName=" + marketWaithandleName
				+ ", marketUpdateSName=" + marketUpdateSid + "]";
	}

	public Integer getMarketId() {
		return marketId;
	}

	public void setMarketId(Integer marketId) {
		this.marketId = marketId;
	}

	public String getMarketCoding() {
		return marketCoding;
	}

	public void setMarketCoding(String marketCoding) {
		this.marketCoding = marketCoding;
	}

	public String getMarketName() {
		return marketName;
	}

	public void setMarketName(String marketName) {
		this.marketName = marketName;
	}

	public String getMarketDate() {
		return marketDate;
	}

	public void setMarketDate(String marketDate) {
		this.marketDate = marketDate;
	}

	public String getMarketStateValue() {
		return marketStateValue;
	}

	public void setMarketStateValue(String marketStateValue) {
		this.marketStateValue = marketStateValue;
	}

	public String getMarketContent() {
		return marketContent;
	}

	public void setMarketContent(String marketContent) {
		this.marketContent = marketContent;
	}

	public String getMarketUpdateDate() {
		return marketUpdateDate;
	}

	public void setMarketUpdateDate(String marketUpdateDate) {
		this.marketUpdateDate = marketUpdateDate;
	}

	public String getMarketCreateDate() {
		return marketCreateDate;
	}

	public void setMarketCreateDate(String marketCreateDate) {
		this.marketCreateDate = marketCreateDate;
	}

	public String getMarketOperatorName() {
		return marketOperatorName;
	}

	public void setMarketOperatorName(String marketOperatorName) {
		this.marketOperatorName = marketOperatorName;
	}

	public String getMarketWaithandleName() {
		return marketWaithandleName;
	}

	public void setMarketWaithandleName(String marketWaithandleName) {
		this.marketWaithandleName = marketWaithandleName;
	}

	public int getMarketUpdateSid() {
		return marketUpdateSid;
	}

	public void setMarketUpdateSid(int marketUpdateSid) {
		this.marketUpdateSid = marketUpdateSid;
	}

	public String getStaffName() {
		return staffName;
	}

	public void setStaffName(String staffName) {
		this.staffName = staffName;
	}
	
	
	
	
}
