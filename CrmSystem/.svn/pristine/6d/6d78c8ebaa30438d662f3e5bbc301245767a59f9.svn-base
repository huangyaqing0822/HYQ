package com.lxit.crmsystem.vo;

public class ViewMonthStatistics {
	/**
	 * 'Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct',
	 * 'Nov', 'dec'
	 */
	private double Apr;
	private double Aug;
	private double Decc;
	private double Feb;
	private double Jan;
	private double Jul;
	private double Jun;
	private double Mar;
	private double May;
	private double Nov;
	private double Oct;
	private double Sep;

	public double getApr() {
		return Apr;
	}

	public void setApr(double apr) {
		Apr = apr;
	}

	public double getAug() {
		return Aug;
	}

	public void setAug(double aug) {
		Aug = aug;
	}

	public double getDecc() {
		return Decc;
	}

	public void setDecc(double decc) {
		Decc = decc;
	}

	public double getFeb() {
		return Feb;
	}

	public void setFeb(double feb) {
		Feb = feb;
	}

	public double getJan() {
		return Jan;
	}

	public void setJan(double jan) {
		Jan = jan;
	}

	public double getJul() {
		return Jul;
	}

	public void setJul(double jul) {
		Jul = jul;
	}

	public double getJun() {
		return Jun;
	}

	public void setJun(double jun) {
		Jun = jun;
	}

	public double getMar() {
		return Mar;
	}

	public void setMar(double mar) {
		Mar = mar;
	}

	public double getMay() {
		return May;
	}

	public void setMay(double may) {
		May = may;
	}

	public double getNov() {
		return Nov;
	}

	public void setNov(double nov) {
		Nov = nov;
	}

	public double getOct() {
		return Oct;
	}

	public void setOct(double oct) {
		Oct = oct;
	}

	public double getSep() {
		return Sep;
	}

	public void setSep(double sep) {
		Sep = sep;
	}

	@Override
	public String toString() {
		return "ViewMonthStatistics [Apr=" + Apr + ", Aug=" + Aug + ", Decc=" + Decc + ", Feb=" + Feb + ", Jan=" + Jan
				+ ", Jul=" + Jul + ", Jun=" + Jun + ", Mar=" + Mar + ", May=" + May + ", Nov=" + Nov + ", Oct=" + Oct
				+ ", Sep=" + Sep + "]";
	}

}
